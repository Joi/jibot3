{
  "name": "jibot3",
  "version": "1.0.0",
  "description": "A Slack Bot",
  "scripts": {
    "ng": "ng",
    "install:all": "npm run install:jibot & npm run install:db & npm run install:python",
    
    "start:db": "cd ./src/db && npm run start",
    "dev:ssr": "ng run jibot3:serve-ssr",
    "build:ssr": "ng build --prod && ng run jibot3:server:production",
    "prod:ssr": "node dist/jibot3/server/main.js",
    "prerender": "ng run jibot3:prerender",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    
    "install:db": "cd src/db && npm install",
    "install:python": "cd src/python && pip3 install -r requirements.txt",
    "install:jibot": "npm install"
    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joi/jibot3.git"
  },
  "keywords": [
    "slack",
    "chat",
    "bot"
  ],
  "author": "jibot team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joi/jibot3/issues"
  },
  "homepage": "https://github.com/Joi/jibot3#readme",
  "dependencies": {
    "@angular/animations": "^10.1.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "^10.1.0",
    "@angular/compiler": "^10.1.0",
    "@angular/core": "^10.1.0",
    "@angular/forms": "^10.1.0",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "^10.1.0",
    "@angular/platform-browser-dynamic": "^10.1.0",
    "@angular/platform-server": "^10.1.0",
    "@angular/router": "^10.1.0",
    "@ng-bootstrap/ng-bootstrap": "^8.0.4",
    "@ng-bootstrap/schematics": "^2.0.0-alpha.1",
    "@nguniversal/express-engine": "^10.1.0",
    "@slack/bolt": "^3.1.1",
    "base64-img": "^1.0.4",
    "bootstrap": "^4.6.0",
    "compromise": "^13.10.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "python-shell": "^2.0.3",
    "rxjs": "~6.6.3",
    "sqlite3": "^5.0.2",
    "ts-loader": "^8.0.14",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "^10.1.0",
    "@angular/compiler-cli": "^10.1.0",
    "@angular/language-service": "^10.1.0",
    "@angular/localize": "^10.2.4",
    "@nguniversal/builders": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "bufferutil": "^4.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typeorm": "^0.2.31",
    "typescript": "~4.0.5",
    "utf-8-validate": "^5.0.4"
  }
}
