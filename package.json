{
  "name": "jibot3",
  "version": "1.0.0",
  "description": "A Slack Bot",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run jibot3:serve-ssr",
    "serve:ssr": "node functions/dist/server/main.js",
    "build:ssr": "ng build --prod && ng run jibot3:server:production",
    "build:functions": "npm run --prefix functions build",
    "build:all": "npm run build:ssr && npm run build:functions",
    "prerender": "ng run jibot3:prerender"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Joi/jibot3.git"
  },
  "keywords": [
    "slack",
    "chat",
    "bot"
  ],
  "author": "jibot team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Joi/jibot3/issues"
  },
  "homepage": "https://github.com/Joi/jibot3#readme",
  "dependencies": {
    "@angular/animations": "~11.1.1",
    "@angular/common": "~11.1.1",
    "@angular/compiler": "~11.1.1",
    "@angular/core": "~11.1.1",
    "@angular/fire": "^6.1.4",
    "@angular/forms": "~11.1.1",
    "@angular/platform-browser": "~11.1.1",
    "@angular/platform-browser-dynamic": "~11.1.1",
    "@angular/platform-server": "^11.1.1",
    "@angular/router": "~11.1.1",
    "@nguniversal/express-engine": "^11.1.1",
    "@slack/bolt": "^3.1.1",
    "express": "^4.17.1",
    "firebase": "^7.0 || ^8.0",
    "rxjs": "~6.6.3",
    "ts-loader": "^8.0.14",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.2",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "~11.1.1",
    "@angular/language-service": "~11.1.1",
    "@nguniversal/builders": "^11.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
